<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.helper.DESFactory">
   <children>
      <Button mnemonicParsing="false" onAction="#generateDes" prefHeight="41.0" prefWidth="101.0" stylesheets="@../css/style.css" text="Generate">
         <styleClass>
            <String fx:value="btn" />
            <String fx:value="edgy" />
            <String fx:value="grey" />
         </styleClass>
         <font>
            <Font name="Agency FB" size="19.0" />
         </font>
      </Button>
      <Label fx:id="desKey">
         <font>
            <Font name="Agency FB" size="23.0" />
         </font>
      </Label>
      <Label fx:id="desWarning" text="please generate a des" textFill="#a10000" visible="false">
         <font>
            <Font name="Agency FB" size="16.0" />
         </font>
      </Label>
   </children>
</VBox>
